programa {
	funcao inicio() {
		    inteiro contador, numero, resultado
		    contador = 1
		
		escreva("Digite o valor para tabuada:")
		leia(numero)
		faca{
		    resultado = contador * numero
		    escreva(numero, " x ", contador, " = ", resultado, "\n")
		    contador = contador +1
		}enquanto(contador <= 10)
	}
}
