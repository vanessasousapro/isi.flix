programa {
	funcao inicio() {
		cadeia  nome
		inteiro numero1
		real    numero2
		escreva("por favor, digite o seu nome\n")
		leia(nome)
		escreva("o nome digitado foi: ", nome, "\n")
		escreva("por favor, digite um número inteiro\n")
